{
  "name": "ontodia",
  "version": "0.12.0",
  "description": "Ontodia Library",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sputniq-space/ontodia.git"
  },
  "keywords": [
    "owl",
    "graph",
    "rdf",
    "diagram",
    "sparql"
  ],
  "author": {
    "name": "ontodia.org",
    "url": "http://ontodia.org"
  },
  "license": "LGPL-2.1",
